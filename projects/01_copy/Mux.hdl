/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
	Not(in=sel,out=t1);
	And(a=a,b=t1,out=t2);
	And(a=b,b=sel,out=t3);
	Or(a=t2,b=t3,out=out);	
}
